<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads latest version of DisplayLink USB Graphics Software.</string>
    <key>Identifier</key>
    <string>com.github.hitachivantara.jss.displaylinkdrivers</string>
    <key>Input</key>
    <dict>
       <key>NAME</key>
       <string>DisplayLink USB Graphics Software</string>
       <key>prod_name</key>
       <string>%NAME%</string>
       <key>category</key>
       <string>Utilities</string>
    </dict>
    <key>MinimumVersion</key>
    <string>1.4.0</string>
    <key>ParentRecipe</key>
    <string>com.github.hitachivantara.pkg.displaylinkdrivers</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>JSSImporter</string>
            <key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%-%version%.pkg</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
